{
  "name": "@norskvideo/norsk-studio-alpha",
  "version": "1.10.0",
  "description": "",
  "scripts": {
    "clean": "rm -rf lib",
    "types": "npx openapi-typescript src/processor.monetise/types.yaml -o src/processor.monetise/types.ts",
    "build": "npm run types && npx studio-wrap-infos src/ && npx npm-run-all build:* && rm -rf build",
    "build:server": "npx tsc",
    "build:client": "npx tsc -p tsconfig-client.json && npx studio-bundle build/info.js client/info.js",
    "build:copy-css": "rsync --mkpath -v -r --include '*/' --include '*.css' --exclude '*' src/ client/",
    "build:types": "rsync --mkpath -v -r --include '*/' --include 'types.yaml' --exclude '*' src/ lib/",
    "build:tailwind": "npx tailwindcss -i src/shared/tailwind.css -o client/style.css",
    "build:eslint": "npx eslint src",
    "test": "LOG_LEVEL=error mocha  --no-parallel --jobs=0 --timeout 120000 --recursive lib/test"
  },
  "files": [
    "client/*",
    "lib/*",
    "shared/*"
  ],
  "license": "MIT",
  "dependencies": {
    "@norskvideo/norsk-sdk": "^1.0.400-2025-02-17-bf239459",
    "@norskvideo/norsk-studio": "^1.20.1-2025-02-17-0ec21693",
    "openapi-types": "^12.1.3"
  },
  "main": "lib/index.js",
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/node-fetch": "^2.6.9",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.9.1",
    "chai": "^4.3.10",
    "esbuild": "^0.20.2",
    "esbuild-plugin-external-global": "^1.0.1",
    "eslint": "^8.53.0",
    "hls-parser": "^0.10.8",
    "JSX": "^1.1.0",
    "mocha": "^10.4.0",
    "node-fetch": "^2.7.0",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^21.5.2",
    "react": "^18.3.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.5"
  }
}
