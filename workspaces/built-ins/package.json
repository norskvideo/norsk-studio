{
  "name": "@norskvideo/norsk-studio-built-ins",
  "version": "1.16.0",
  "description": "",
  "scripts": {
    "clean": "rm -rf lib",
    "types": "npx openapi-typescript src/processor.onscreenGraphic/types.yaml -o src/processor.onscreenGraphic/types.ts && npx openapi-typescript src/processor.browserOverlay/types.yaml -o src/processor.browserOverlay/openApi.ts     ",
    "build": "npm run types && npx studio-wrap-infos src/ && npx npm-run-all build:* && rm -rf build",
    "build:server": "npx tsc",
    "build:client": "npx tsc -p tsconfig-client.json && npx studio-bundle build/info.js client/info.js",
    "build:css": "rsync --mkpath -v -r --include '*/' --include '*.css' --exclude '*' src/ client/",
    "build:types": "rsync --mkpath -v -r --include '*/' --include 'types.yaml' --exclude '*' src/ lib/",
    "build:tailwind": "npx tailwindcss -i src/shared/tailwind.css -o client/style.css",
    "build:eslint": "npx eslint src",
    "test": "LOG_LEVEL=${LOG_LEVEL:-error} NODE_ENV=test mocha  --no-parallel --jobs=0 --timeout 120000 --recursive lib/test"
  },
  "files": [
    "client/*",
    "lib/*",
    "shared/*"
  ],
  "license": "MIT",
  "dependencies": {
    "@norskvideo/norsk-sdk": "^1.0.383",
    "@norskvideo/norsk-studio": "^1.24.0",
    "@norskvideo/webrtc-client": "^0.1.2",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-ws": "^5.0.2",
    "json-refs": "^3.0.15",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1"
  },
  "main": "lib/index.js",
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.20",
    "@types/express-ws": "^3.0.3",
    "@types/hls-parser": "^0.8.7",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/mocha": "^10.0.6",
    "@types/multer": "^1.4.11",
    "@types/node-fetch": "^2.6.9",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.9.1",
    "chai": "^4.3.10",
    "chart.js": "^4.4.2",
    "esbuild": "^0.20.2",
    "esbuild-plugin-external-global": "^1.0.1",
    "eslint": "^8.53.0",
    "hls-parser": "^0.10.8",
    "hls.js": "^1.5.8",
    "JSX": "^1.1.0",
    "mocha": "^10.4.0",
    "node-fetch": "^2.7.0",
    "npm-run-all": "^4.1.5",
    "openapi-types": "^12.1.3",
    "openapi-typescript": "^7.3.0",
    "puppeteer": "^21.5.2",
    "react": "^18.3.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.5",
    "yaml": "^2.3.3"
  }
}
